<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".util.PdfViewActivityStudent">

    <androidx.appcompat.widget.Toolbar
        android:background="@color/back_centerl"
        app:title="Time Table"
        android:id="@+id/v_ttable_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleTextColor="@color/white"

        />

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:id="@+id/v_swipe_reload"
        android:layout_below="@+id/v_ttable_bar"
        android:layout_height="match_parent">



        <com.github.barteksc.pdfviewer.PDFView
            android:layout_width="match_parent"
            android:id="@+id/v_pdfView"
            android:layout_height="match_parent" />




    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>



</RelativeLayout>